<?xml version="1.0"?>
<extension key="de.systopia.revent" type="module">
  <file>revent</file>
  <name>Remote Events</name>
  <description>Tools for remote event registrations</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
